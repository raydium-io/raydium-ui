    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.4.4",
    "vue-jest": "^3.0.4"
